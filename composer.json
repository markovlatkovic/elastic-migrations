{
    "name": "babenkoivan/elastic-migrations",
    "description": "Elasticsearch migrations for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Babenko",
            "email": "babenko.i.a@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ElasticMigrations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElasticMigrations\\Tests\\": "tests"
        }
    },
    "require": {
        "babenkoivan/elastic-client": "dev-master",
        "babenkoivan/elastic-adapter": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^4.4"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ElasticMigrations\\ServiceProvider"
            ]
        }
    }
}
